{
    "extends": "xo-react",
    "envs": [
        "browser",
        "es2020",
        "jest"
    ],
    "space": 4,
    "prettier": true,
    "rules": {
        "node/file-extension-in-import": [
            "off"
        ],
        "import/extensions": [
            "error",
            "never",
            {
                "jpg": "always",
                "css": "always",
                "json": "always"
            }
        ]
    },
    "parser": "@babel/eslint-parser"
}